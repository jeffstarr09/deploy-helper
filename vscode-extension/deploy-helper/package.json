{
  "name": "deploy-helper",
  "displayName": "deploy-helper",
  "description": "VS Code extension for Deploy Helper web app integration",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.85.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "deploy-helper.processCode",
        "title": "Deploy Helper: Process Code"
      },
      {
        "command": "deploy-helper.checkUpdates",
        "title": "Deploy Helper: Check for Updates"
      },
      {
        "command": "deploy-helper.showStatus",
        "title": "Deploy Helper: Show Status"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.6",
    "@types/node": "18.x",
    "@types/vscode": "^1.85.0",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "@vscode/test-cli": "^0.0.4",
    "@vscode/test-electron": "^2.3.8",
    "eslint": "^8.56.0",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "firebase": "^10.7.1"
  }
}